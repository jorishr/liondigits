{
  "languages": {
    "base": "nl",
    "targets": ["en", "es", "ca"]
  },
  "languageFile": {
    "prefix": "txt_data_"
  },
  "mode": {
    "backup": true,
    "translate": true,
    "dryRun": false,
    "logLevel": "verbose"
  },
  "id": {
    "textNodeId": "data-txt_id",
    "attributeTextId": {
      "altId": "data-txt_id__alt",
      "titleId": "data-txt_id__title",
      "placeholderId": "data-txt_id__placeholder",
      "metaId": "data-txt_id__meta"
    }
  },
  "keyCountOffset": 100,
  "customKeys": ["702"],
  "elements": {
    "defaultSelectors": [
      "head meta[name=description]",
      "head meta[name=keywords]",
      "head title",
      "h1",
      "h2",
      "h3",
      "h4",
      "h5",
      "h6",
      "p",
      "span",
      "a",
      "strong",
      "em",
      "small",
      "cite",
      "abbr",
      "img",
      "picture",
      "input",
      "button"
    ],
    "defaultAttributes": ["alt", "title", "placeholder", "meta"],
    "addSelectors": [],
    "exclude": {
      "defaultSelectorsToExclude": [],
      "classToExclude": [],
      "idToExclude": []
    }
  },
  "folders": {
    "src": "./app",
    "dest": "./app/data",
    "backup": "./.backup"
  },
  "textUpdateDirection": "default"
}
